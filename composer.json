{
    "name": "yanepenb/page-analyzer",
    "description": "Page-analyzer can help you .. .",
    "keywords": ["page", "analyzer", "laravel", "lumen"],
    "authors": [
        {
            "name": "Ilya Sharipov",
            "email": "eanepenb@mail.ru"
        }
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.3",
        "laravel/lumen-framework": "^6.0",
        "nesbot/carbon": "^2.27"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "phpcs": "phpcs",
        "phpunit": "phpunit",
        "test": "phpunit tests"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
